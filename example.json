        "/core_user/preferences/{preference}": {
            "get": {
                "description": "Fetch one user preference, or all user preferences",
                "summary": "Fetch user preferences",
                "tags": [
                    "core_user"
                ],
                "parameters": [
                    {
                        "name": "preference",
                        "in": "path",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "headers": {
                            "X-Total-Count": {
                                "$ref": "#/components/headers/core_user\\route\\headers\\example_header"
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/core\\router\\schema\\objects\\array_of_strings"
                                },
                                "examples": {
                                    "A single preference value": {
                                        "summary": "A json response containing a single preference",
                                        "value": {
                                            "drawers-open-index": "1"
                                        }
                                    },
                                    "A set of preference values": {
                                        "summary": "A json response containing a set of preferences",
                                        "value": {
                                            "drawers-open-index": "1",
                                            "login_failed_count_since_success": "1",
                                            "coursesectionspreferences_2": "{\"contentcollapsed\":[]}"
                                        }
                                    }
                                }
                            },
                            "application/xml": {
                                "schema": {
                                    "$ref": "#/components/schemas/core\\router\\schema\\objects\\array_of_strings"
