    "paths": {
        "/core/templates/{themename}/{component}/{identifier}": {
            "get": {
                "description": "Fetch a single template for a component in a theme",
                "summary": "Fetch a single template",
                "tags": [
                    "core"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/core\\router\\parameters\\path_themename"
                    },
                    {
                        "$ref": "#/components/parameters/core\\router\\parameters\\path_component"
                    },
                    {
                        "name": "identifier",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "pattern": "^[a-zA-Z0-9\\/_\\-]*$"
                        },
                        "required": true
                    },
                    {
                        "name": "includecomments",
                        "in": "query",
                        "description": "Include comments in the template",
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/core\\router\\schema\\objects\\array_of_strings"
                                },
                                "examples": {
                                    "Single template value": {
                                        "summary": "A json response containing the template for a single template",
                                        "value": {
                                            "mod_example/template_identifier": "<div class=\"example\">Hello World</div>"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "$ref": "#/components/responses/core\\router\\response\\invalid_parameter_response"
                    }
                },
                "security": []
            }
        },
