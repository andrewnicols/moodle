        "parameters": {
            "core\\router\\parameters\\path_themename": {
                "name": "themename",
                "in": "path",
                "description": "The name of a Moodle theme.",
                "schema": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9_\\-]*$"
                },
                "examples": {
                    "The Boost theme": {
                        "value": "boost"
                    },
                    "The Classic theme": {
                        "value": "classic"
                    }
                },
                "required": true
            },
            "core\\router\\parameters\\path_component": {
                "name": "component",
                "in": "path",
                "description": "The name of a Moodle component, in frankenstyle format.",
                "schema": {
                    "type": "string",
                    "pattern": "^[a-z][a-z0-9]*(_(?:[a-z][a-z0-9_](?!__))*)?[a-z0-9]+$"
                },
                "examples": {
                    "The core subsystem": {
                        "value": "core"
                    },
                    "The Course subsystem": {
                        "value": "core_course"
                    },
                    "An activity module": {
                        "value": "mod_assign"
                    },
                    "An assignment subplugin": {
                        "value": "assignsubmission_file"
                    }
                },
                "required": true
            },
            "core\\router\\parameters\\course_parameter": {
                "name": "course",
                "in": "path",
                "description": "The course identifier.\n\nThis can be the id of the course, the idnumber of the course, or the shortname of the course.\n\nIf specifying a course idnumber, the value should be in the format `idnumber:[idnumber]`.\n\nIf specifying a course shortname, the value should be in the format `name:[shortname]`.",
                "schema": {
                    "type": "string"
                },
                "examples": {
                    "A course id": {
                        "value": 54
                    },
                    "A course specified by its idnumber": {
                        "value": "idnumber:000117-physics-101-1"
                    },
                    "A course specified by its shortname": {
                        "value": "name:000117-phys101-0"
                    }
                },
                "required": true
            }
        },
